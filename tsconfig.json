{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"]
    },
    // "strictNullChecks": true,
    // "noImplicitThis": true,
    // "noUnusedParameters": true,
    // "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true
  }
